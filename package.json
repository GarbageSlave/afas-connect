{
  "name": "afas-connect",
  "version": "1.0.0",
  "description": "An API that makes it easy to connect to Afas Profit services written in javascript.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarbageSlave/afas-connect.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GarbageSlave/afas-connect/issues"
  },
  "homepage": "https://github.com/GarbageSlave/afas-connect#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Chris Kleijn",
  "keywords": [
    "AFAS",
    "Connect",
    "afas",
    "Afas",
    "connect"
  ],
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
